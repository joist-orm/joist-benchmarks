{
  "name": "benchmark",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "@source": "./src/index.ts",
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "require": {
        "@source": "./src/index.ts",
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    }
  },
  "scripts": {
    "build": "tsc",
    "benchmark-2": "node --loader ts-node/esm --conditions=@source src/benchmark.ts",
    "benchmark": "node --import tsx --conditions=@source src/benchmark.ts",
    "pipeline": "node --expose-gc --import tsx --conditions=@source src/pipeline.ts"
  },
  "dependencies": {
    "benchmark-joist-v1": "workspace:^",
    "benchmark-joist-v2": "workspace:^",
    "benchmark-mikroorm": "workspace:^",
    "benchmark-prisma": "workspace:^",
    "benchmark-typeorm": "workspace:^",
    "cli-table3": "^0.6.5",
    "colors": "^1.4.0",
    "mitata": "^1.0.34",
    "seed-data": "workspace:^"
  },
  "devDependencies": {
    "@types/node": "^22.13.16",
    "typescript": "^5.8.2"
  }
}
