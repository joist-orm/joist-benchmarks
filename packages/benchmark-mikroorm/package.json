{
  "name": "benchmark-mikroorm",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "@source": "./src/index.ts",
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "require": {
        "@source": "./src/index.ts",
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    }
  },
  "scripts": {
    "build": "tsc",
    "bench": "node --import=tsx --conditions=@source src/index.ts"
  },
  "dependencies": {
    "@mikro-orm/core": "^6.4.11",
    "@mikro-orm/postgresql": "^6.4.11",
    "@mikro-orm/reflection": "^6.4.11",
    "benchmark": "workspace:^",
    "pg": "^8.14.1",
    "seed-data": "workspace:^"
  },
  "devDependencies": {
    "@types/node": "^22.13.14",
    "typescript": "^5.8.2"
  }
}
